<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>

  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div class="actions">
    <%= form.submit "ログイン",class: "btn btn-primary" %>
  </div>
<% end %>
